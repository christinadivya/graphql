{
  "name": "webservice-boilerplate",
  "version": "0.0.1",
  "description": "NodeJS TypeScript Webservice boilerplate",
  "main": "index.js",
  "private": true,
  "engines": {
    "node": ">=16.16.0",
    "npm": ">=8.15.1"
  },
  "scripts": {
    "lint:fix": "eslint --color --quiet --fix --ext .ts,.js .",
    "dev:lint:fix": "nodemon -e ts -x npm run lint:fix",
    "dev:prettier": "prettier --write .",
    "dev:tsc": "tsc",
    "copy-env": "cp ./local-setup/local-env .env",
    "start": "ts-node index.ts",
    "start:lint": "eslint --color --quiet --ext .ts,.js .",
    "start:local": "nodemon -w src -w index.ts -e ts,js,json,yml -x babel-node --require dotenv/config --extensions .ts,.js index.ts",
    "dev": "npm run copy-env && npm-run-all -p start:local dev:tsc dev:lint:fix dev:prettier",
    "typeorm": "typeorm-ts-node-commonjs",
    "typeorm_src": "typeorm-ts-node-commonjs -d src/data-source.ts",
    "migration": "npm run typeorm_src migration:run",
    "seed:config": "babel-node --extensions .ts,.js ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed": "babel-node --extensions .ts,.js ./node_modules/typeorm-seeding/dist/cli.js seed",
    "local": "npm run copy-env && docker-compose -f ./local-setup/docker-compose.yml up -d",
    "test:unit": "NODE_ENV=test mocha --exit tests/**/*.spec.ts --timeout 8000",
    "test:unit:coverage": "nyc --reporter=lcov --report-dir=./coverage/unit --check-coverage npm run test:unit",
    "test": "npm run test:unit:coverage",
    "sonar": "sonar-scanner",
    "postinstall": "patch-package"
  },
  "keywords": [
    "express",
    "node",
    "mysql",
    "REST",
    "API",
    "docker",
    "swagger",
    "eslint",
    "boilerplate"
  ],
  "dependencies": {
    "@bull-board/express": "^4.2.2",
    "@jorgebodega/typeorm-seeding": "^5.0.1",
    "@sentry/node": "^7.11.1",
    "@sentry/tracing": "^7.11.1",
    "bluebird": "^3.7.2",
    "bull": "^4.8.5",
    "bullmq": "^1.89.2",
    "chalk": "^4.1.2",
    "chance": "^1.1.8",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-validation": "^4.1.0",
    "form-data": "^4.0.0",
    "helmet": "^5.1.1",
    "http-status-code": "^2.1.0",
    "i18next-fs-backend": "^1.1.5",
    "i18next-http-middleware": "^3.2.1",
    "ioredis": "^5.2.3",
    "joi-password-complexity": "^5.1.0",
    "jsonwebtoken": "^8.5.1",
    "limiter": "^2.1.0",
    "log4js": "^6.6.1",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "nanoid": "^4.0.0",
    "patch-package": "^6.4.7",
    "prom-client": "^14.1.0",
    "redis": "^4.3.1",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.5.1",
    "swagger-ui-express": "^4.5.0",
    "typeorm": "0.3.7",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/node": "^7.18.10",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.18.10",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@types/body-parser": "^1.19.2",
    "@types/bull": "^3.15.9",
    "@types/chai-as-promised": "^7.1.5",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/i18n": "^0.13.4",
    "@types/i18next-fs-backend": "^1.1.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mocha": "^9.1.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.10",
    "@types/sinon": "^10.0.13",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "chai-exclude": "^2.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.22.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-tsdoc": "^0.2.16",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.19",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.1.0",
    "sinon": "^14.0.0",
    "sonar-scanner": "^3.1.0",
    "ts-node": "10.7.0",
    "typescript": "4.5.2"
  }
}
